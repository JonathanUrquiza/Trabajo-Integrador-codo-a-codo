<%- include('partials/headerS') %>
<main class="container">
    <section class="row">
        <div class="col-8 d-flex align-items-center py-4 bg-body-tertiary form-signin m-auto">
            <form method="post" action="http://demo-registro.com.ar" id="survey-form" enctype="text/plain">
                <img class="mb-4" src="../multimedia/branding/isotype.svg" alt="Icono del sitio web" width="72" height="57">
                <h1 class="h3 mb-3 fw-normal">Por favor Registrese</h1>
            
                <div class="form-floating">
                  <input type="email" class="form-control" name="email" id="floatingInput" placeholder="name@example.com">
                  <label for="floatingInput">Direcion de Email</label>
                </div>
                <div class="form-floating">
                  <input type="password" class="form-control" name="password" id="floatingPassword" placeholder="Password">
                  <label for="floatingPassword">Contraseña</label>
                </div>
            
                <div class="form-check text-start my-3">
                  <input class="form-check-input" type="checkbox" value="remember-me" id="flexCheckDefault">
                  <label class="form-check-label" for="flexCheckDefault">
                    Recuerdame
                  </label>
                </div>
                <button class="btn btn-primary w-100 py-2" type="submit">Acceder</button>
                <p class="mt-5 mb-3 text-body-secondary">© 2017–2023</p>
              </form>
        </div>
    </section>
</main>
<%- include('partials/footerS') %>