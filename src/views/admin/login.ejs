<%- include('../partials/headerS') %>
<main class="container" id="login">
    <h2 class="login__title">INGRESAR A MI CUENTA</h2>
    <form class="login__form form" action="" method="post">
      <div class="form__box--grid">
        <label class="form__label" for="emal">Email:</label>
        <input class="form__input" type="text" name="email" id="email" placeholder="@email.com">
      </div>
      <div class="form__box--grid">
        <label class="form__label" for="password">Contraseña:</label>
        <input class="form__input" type="password" name="password" id="password" placeholder="*******">
      </div>
      <div class="form__subnission">
        <input class="form__btn btn btn--primary btn--large" type="submit" value="INGRESAR">
      </div>
      <div class="form__remember">
        <input type="checkbox" name="remembre" id="">
        <label for="">Recordarme</label>
      </div>
      <a class="form__link" href="">Olvidé mi Contraseña.</a>
    </form>
</main>
<%- include('../partials/footerS') %>